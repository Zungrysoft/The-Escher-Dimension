#Check for the portal to Layer 4
function tid:dungeon/brick/event1_check

#Activate whichever structure block was picked
setblock ~ ~1 ~ minecraft:redstone_block

#Mark this area as generated
fill ~1 ~129 ~1 ~6 ~132 ~6 minecraft:blue_wool

#Replace the hole in the floor
fill ~1 ~ ~1 ~2 ~ ~1 brick_stairs[facing=north]
fill ~1 ~ ~2 ~2 ~ ~5 air
fill ~5 ~ ~6 ~6 ~ ~6 brick_stairs[facing=south]
fill ~5 ~ ~2 ~6 ~ ~5 air

#Count the room in the total
scoreboard players remove brick_rooms value 1

#We're done here
kill @s

